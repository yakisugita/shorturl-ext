{
    "name": "短縮リンク作成",
    "version": "2.0",
    "manifest_version": 3,
    "description": "自作短縮リンクのChrome拡張 複数のURLをまとめて短縮できます",
    "icons": {
        "48": "icon3.png"
    },
    "background": {
        "service_worker": "contextmenu.js",
        "type": "module"
    },
    "permissions": [
        "tabs",
        "storage",
        "contextMenus",
        "notifications"
    ],
    "action": {
        "default_icon": {
            "19": "icon3.png"
        },
        "default_popup": "popup.html"
    },
    "homepage_url": "https://8jsv.com/"
}