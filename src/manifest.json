{
    "name": "短縮リンク作成",
    "version": "1.3.1",
    "manifest_version": 3,
    "description": "短縮リンクを簡単に作成できます。カスタムリンクも使用可能。",
    "icons": {
        "48": "icon3.png"
    },
    "background": {
        "service_worker": "contextmenu.js",
        "type": "module"
    },
    "permissions": [
        "tabs",
        "storage",
        "contextMenus",
        "notifications"
    ],
    "action": {
        "default_icon": {
            "19": "icon3.png"
        },
        "default_popup": "popup.html"
    },
    "homepage_url": "https://twitter.com/_yakisugita_"
}