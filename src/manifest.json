{
    "name": "短縮リンク作成",
    "version": "2.0",
    "manifest_version": 3,
    "description": "自作短縮リンクのFirefoxアドオン 複数のURLをまとめて短縮できます",
    "icons": {
        "48": "icon3.png"
    },
    "background": {
        "page": "firefoxbg.html"
    },
    "permissions": [
        "tabs",
        "storage",
        "contextMenus",
        "notifications",
        "clipboardWrite"
    ],
    "action": {
        "default_icon": {
            "19": "icon3.png"
        },
        "default_popup": "popup.html"
    },
    "homepage_url": "https://8jsv.com/",
    "browser_specific_settings": {
        "gecko": {
          "id": "{e224f9a5-49c0-479c-beb5-599757160ff8}"
        }
      }
}