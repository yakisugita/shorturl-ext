{
    "name": "短縮リンク作成 for Firefox",
    "version": "1.2.1",
    "manifest_version": 3,
    "description": "短縮リンクを簡単に作成できます。カスタムリンクも使用可能。",
    "icons": {
        "48": "icon3.png"
    },
    "background": {
        "page": "firefoxbg.html"
    },
    "permissions": [
        "tabs",
        "storage",
        "contextMenus",
        "notifications",
        "clipboardWrite"
    ],
    "action": {
        "default_icon": {
            "19": "icon3.png"
        },
        "default_popup": "popup.html"
    },
    "homepage_url": "https://twitter.com/_yakisugita_",
    "browser_specific_settings": {
        "gecko": {
          "id": "{e224f9a5-49c0-479c-beb5-599757160ff8}",
          "strict_min_version": "42.0"
        }
      }
}